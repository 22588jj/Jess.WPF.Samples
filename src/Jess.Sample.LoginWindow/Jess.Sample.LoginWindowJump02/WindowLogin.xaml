<Window x:Class="Jess.Sample.LoginWindowJump.WindowLogin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Jess.Sample.LoginWindowJump"
        mc:Ignorable="d" WindowStartupLocation="CenterScreen"
        Title="登录" Height="200" Width="300">
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Vertical">
        <Button Content="登录"  Click="Btn_Login_Click"/>
        <Button Content="取消"  Click="Btn_Cancel_Click"/>

    </StackPanel>
</Window>
